CC = gcc
CXX = g++

INCLUDES = -I../mylist
CFLAGS = -g -Wall $(INCLUDES)
CXXFLAGS = -g -Wall $(INCLUDES)

LDFlAGS = -g -L../mylist
LDLIBS = -lmylist

PHONY: default
default: mdb-add mdb-lookup

mdb-lookup: mdb.o

mdb-add: mdb.o

mdb-add.o: mdb.h

mdb-lookup.o: mdb.h

mdb.o: mdb.h

.PHONY: clean
clean: 
	rm -f *.o *~ a.out core mdb-add mdb-lookup

.PHONY: all
all: clean default
