CC = gcc
CXX = g++

INCLUDES = -I../mylist
CFLAGS = -g -Wall $(INCLUDES)
CXXFLAGS = -g -Wall $(INCLUDES)

LDFLAGS = -g -L../mylist
LDLIBS = -lmylist

./PHONY: default
default: mdb-lookup-server

mdb-lookup-server.o: mdb.h

.PHONY: clean
clean:
	rm -f *.o *~ a.out core mdb-lookup-server

./PHONY: all
all: clean default
